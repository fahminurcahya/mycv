        <div class="resume__right">
            <!--========== EXPERIENCE ==========-->
            <section class="experience section" id="experience">
                <h2 class="section-title-secondary">Experience</h2>

                <div class="experience__container bd-grid">
                    <%for (let i = 0; i < data.experience.length; i++) {%>
                    <div class="experience__content">
                        <div class="experience__time">
                            <span class="experience__rounder"></span>
                            <% if (i < data.experience.length - 1) { %>
                            <span class="experience__line"></span>
                            <% } %>
                        </div>
                        <div class="experience__data bd-grid">
                            <h3 class="experience__title s3"><%=data.experience[i].position%></h3>
                            <span class="experience__company">
                                <h5><%=data.experience[i].period%> </h5><br>
                                <i><%=data.experience[i].client%></i>
                            </span>
                            <p class="experience__description s1">
                                <% for (let j = 0; j < data.experience[i].jobdesk.length; j++ ) { %>
                                - <%=data.experience[i].jobdesk[j]%>
                                <br>
                                <% } %>
                            </p>
                        </div>
                    </div>
                    <% } %>
                </div>

            </section>

