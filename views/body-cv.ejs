<main class="l-main bd-container">
    <!-- All elements within this div, is generated in PDF -->
    <div class="resume" id="area-cv">
        <div class="resume__left">
            <!--========== HOME ==========-->
            <section class="home " id="home">
                <div class="home__container section bd-grid">
                    <div class="home__data bd-grid">
                        <img src="<%=baseUrl + data.img%>" alt="" class="home__img">
                        <h1 class="home__title"><b><%= data.name %></b></h1>
                        <h3 class="home__profession"><%= data.title %></h3>
                        <div>
                            <a href="" id="resume-button" class="home__button home__button-movil">Download</a>
                        </div>
                    </div>
                    <div class="home__address bd-grid">
                        <span class="home__information">
                            <i class="bx bx-map home__icon"></i> <%=data.address%>
                        </span>
                        <span class="home__information">
                            <i class="bx bx-envelope home__icon"></i> <%=data.email%> &nbsp
                        </span>
                        <span class="home__information">
                            <i class="bx bxl-whatsapp home__icon"></i> +<%=data.phone%>
                        </span>
                    </div>
                    <div class="bd-grid">
                        <button class="whatsapp__button" onclick="copyEmail()" style="text-align: center; padding: .5rem .2rem;
                                "><i class="bx bx-copy button__icon"></i>Copy Email</button>
                        <a href="https://wa.me/<%=data.phone%>" class="whatsapp__button" style="text-align: center; padding: .5rem .2rem;
                                "><i class="bx bxl-whatsapp button__icon"></i> Send Whatsapp</a>
                    </div>
                </div>

                <!-- Theme change button -->
                <i class="bx bx-moon change-theme" title="Theme" id="theme-button"></i>

                <!-- Button to generate and download the pdf. Available for desktop. -->
                <!-- <i class="generate-pdf bx bx-download" id="resume-button" title="Generate PDF"></i> -->

            </section>
            <!--========== PROFILE ==========-->
            <section class="profile section" id="profile">
                <h2 class="section-title">PROFILE</h2>
                <p class="profile__description">
                    <% for(let i = 0; i<data.profile.length; i++){%>
                    <%=data.profile[i]%>
                    <% if (i < data.profile.length -1) {%>
                    <br>
                    <%}%>
                    <%}%>
                </p>
            </section>

            <!--========== SOCIAL ==========-->
            <section class="social section">
                <h2 class="section-title">SOCIAL</h2>
                <div class="social__container bd-grid">
                    <a href="<%=data.social[0].url%>" target="__blank" class="social__link">
                        <i class="bx <%=data.social[0].className%> social__icon"></i><%=data.social[0].username%>
                    </a>
                    <a href="<%=data.social[1].url%>" target="__blank" class="social__link">
                        <i class="bx <%=data.social[1].className%> social__icon"></i><%=data.social[1].username%>
                    </a>
                    <a href="<%=data.social[2].url%>" target="__blank" class="social__link">
                        <i class="bx <%=data.social[2].className%> social__icon"></i><%=data.social[2].username%>
                    </a>
                </div>
            </section>



            <!--========== EDUCATION ==========-->
            <section class="education section" id="education">
                <h2 class="section-title">EDUCATION</h2>
                <div class="education__container bd-grid">
                    <div class="education__content">
                        <div class="education__time">
                            <div class="education__rounder"></div>
                        </div>
                        <div class="education__data bd-grid">
                            <h3 class="education__title"><%=data.education.title%></h3>
                            <div class="education__studies"><%=data.education.studies%></div>
                            <div class="education__year"><%=data.education.year%></div>
                        </div>
                    </div>
                </div>

            </section>


